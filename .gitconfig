[remote "cube-js"]
    url = git@github.com:cube-js/cube.js.git
    fetch = +refs/heads/*:refs/remotes/cube-js/*
[remote "origin"]
    url = git@github.com:huangyingw/cube-js_cube.js.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = cube-js
    branch = master
[merge]
    allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = *.gdio.diff
    ignore = .gitignore
    ignore = d3-dashboard/*
    ignore = demo-app/*
    ignore = docker-compose.yml
    ignore = ecom-dump-d3-example.sql
    ignore = ecom-dump.sql
    ignore = install.sh
    ignore = readme
[deploy]
	host = localhost
	path = ~/myproject/git/API_platform/cube-js/cube.js
